{
  "functions": {
    "api/telegram.js": {
      "runtime": "@vercel/node@3.0.0",
      "maxDuration": 10
    },
    "api/health.js": {
      "runtime": "@vercel/node@3.0.0",
      "maxDuration": 10
    },
    "api/posts.js": {
      "runtime": "@vercel/node@3.0.0",
      "maxDuration": 10
    },
    "api/test-firebase.js": {
      "runtime": "@vercel/node@3.0.0",
      "maxDuration": 10
    },
    "api/debug-env.js": {
      "runtime": "@vercel/node@3.0.0",
      "maxDuration": 10
    },
    "api/simple-firebase-test.js": {
      "runtime": "@vercel/node@3.0.0",
      "maxDuration": 10
    }
  },
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    },
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Referrer-Policy",
          "value": "origin-when-cross-origin"
        }
      ]
    }
  ]
}
